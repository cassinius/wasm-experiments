{
  "name": "ts-wasm",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node --experimental-modules --experimental-wasm-modules ./dist/index.js",
    "build": "npm run asbuild && tsc -p ./tsconfig.json && npm run copy",
    "build:watch": "onchange -i 'assembly/**/*' -- npm run build",
    "copy": "rm -rf dist/build && cp -r build dist/",
    "test": "tsc tests/index.ts && npm run test:run",
    "test:run": "node --experimental-wasm-bigint tests/index.js",
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --validate --sourceMap --debug",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --validate --sourceMap --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "author": "",
  "license": "LicenseRef-LICENSE",
  "dependencies": {
    "@assemblyscript/loader": "^0.9.2",
    "@types/node": "^13.7.6",
    "assemblyscript": "^0.9.2",
    "assert": "^2.0.0",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "assemblyscript": "^0.9.2",
    "onchange": "^6.1.0"
  }
}
